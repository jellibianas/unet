{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span>
            SEO

            {% if collector.data.errors > 0 %}
                <span class="sf-toolbar-status sf-toolbar-status-red">{{ collector.data.errors }}</span>
            {% endif %}
            {# <img width="28" height="28" src="data:image/png;base64," alt="SymEdit" /> #}
        </span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Title:</b>
            <span>{{ collector.data.title|default('No Title!') }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Description:</b>
            <span>{{ collector.data.description|default('No Description!') }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Keywords:</b>
            <span>{{ collector.data.keywords }}</span>
        </div>
        {% if collector.data.metas|length > 0 %}
        <div class="sf-toolbar-info-piece">
            <b>Metas:</b>
            {% for type, value in collector.data.metas %}
            <br><b>{{ type }}:</b>

            <table>
                {% for key, value in collector.data.metas[type] %}
                <tr>
                    <td>{{ key }}</td>
                    <td>{{ value|default('(blank)') }}</td>
                </tr>
                {% endfor %}
            </table>
            {% endfor %}
        </div>
        {% endif %}
    {% endset %}

    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with {'link': true } %}
{% endblock %}

{% block menu %}
<span class="label">
    <strong>SEO</strong>
</span>
{% endblock %}

{% block panel %}
Panelll
{% endblock %}
