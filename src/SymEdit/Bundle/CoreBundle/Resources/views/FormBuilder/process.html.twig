{% extends '@SymEdit/Page/base.html.twig' %}

{% block title %}{{ form_builder.legend }}{% endblock %}

{% block heading %}<h1>{{ block('title') }}</h1>{% endblock %}

{% block content %}
    {{ form_start(form) }}
    {{ form_widget(form) }}

    <div class="form-group">
        <div class="col-md-offset-3 col-md-9">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
    </div>

    {{ form_end(form) }}

{% endblock %}