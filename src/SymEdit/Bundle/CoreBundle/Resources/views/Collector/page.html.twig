{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
{% if collector.page %}
    {% set icon %}
        <span>
            <img width="28" height="28" src="data:image/png;base64," alt="SymEdit" />
            <span class="sf-toolbar-status sf-toolbar-info-piece-additional">{{ collector.page.path }}</span>
        </span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>ID:</b>
            <span>{{ collector.page.id }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Path:</b>
            <span>{{ collector.page.path }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Title:</b>
            <span>{{ collector.page.title }}</span>
        </div>
        {% if collector.page.pageController %}
        <div class="sf-toolbar-info-piece">
            <b>Page Controller:</b>
            <span>{{ collector.page.pageControllerPath }}</span>
        </div>
        {% endif %}
    {% endset %}

    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with {'link': true } %}
{% endif %}
{% endblock %}

{% block menu %}
{% if collector.page is not null %}
<span class="label">
    <span class="icon"><img src="data:image/png;base64," alt="SymEdit" /></span>
    <strong>SymEdit</strong>
</span>
{% endif %}
{% endblock %}

{% block panel %}
{% if collector.page %}
    <h2>SymEdit Page</h2>
    <table>
        <tr>
            <td>ID:</td>
            <td>{{ collector.page.id }}</td>
        </tr>

        <tr>
            <td>Path:</td>
            <td>{{ collector.page.path }}</td>
        </tr>

        <tr>
            <td>Title:</td>
            <td>{{ collector.page.title }}</td>
        </tr>

        {% if collector.page.pageController %}
        <tr>
            <td>Controller Path:</td>
            <td>{{ collector.page.pageControllerPath }}</td>
        </tr>

        <tr>
            <td>Controller</td>
            <td>{{ collector.controller }}</td>
        </tr>
        {% else %}
        <tr>
            <td>Template:</td>
            <td>@SymEdit/Page/{{ collector.page.template }}</td>
        </tr>
        {% endif %}
    </table>
{% endif %}
{% endblock %}
